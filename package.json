{
  "name": "bs-theme",
  "version": "1.0.0",
  "description": "A custom WordPress theme for BitcoinChaser.com",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "wp-scripts build",
    "build:css": "sass assets/css:build --no-source-map --style=compressed",
    "watch:css": "sass --watch assets/css:build --no-source-map --style=compressed",
    "start": "wp-scripts start",
    "dev": "npm-run-all --parallel watch:css start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ross Findlay",
  "license": "ISC",
  "dependencies": {
    "@wordpress/block-editor": "^15.3.0",
    "@wordpress/blocks": "^15.3.0",
    "@wordpress/components": "^30.3.0",
    "@wordpress/element": "^6.30.0",
    "@wordpress/scripts": "^30.19.0",
    "bootstrap-icons": "^1.13.1",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "typescript": "^5.9.2"
  }
}